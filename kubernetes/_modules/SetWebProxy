SetWebProxy() {
	_HTTP_PROXY=${HTTP_PROXY}
	_HTTPS_PROXY=${HTTPS_PROXY}
	_NO_PROXY=${NO_PROXY}

	echo -e "\nSetting HTTP_PROXY to '${WEB_PROXY}'"
	export HTTP_PROXY=${WEB_PROXY}

	echo -e "\nSetting HTTPS_PROXY to '${WEB_PROXY}'"
	export HTTPS_PROXY=${WEB_PROXY}

	printf -v _DEV_NO_PROXY '%s,' 10.217.128.{25,26,27,35} 172.17.{64,65}.{25,26,27,35,205} 10.217.76.{25,26,27,35} 172.17.65.{140,141,142,150} 172.17.64.{205,215}
	printf -v _QA_NO_PROXY '%s,' 10.217.88.{152,153,154,162} 10.217.50.{15,16,17,25} 10.217.50.{42,43,44,25} 10.217.{52,84}.{25,26,27,35} 10.217.128.{148,149,150,158} 10.217.64.{25,26,27,35} 10.217.129.{20,21,22,30} 10.217.50.{116,106,107,108}

	echo -e "\nSetting NO_PROXY to '${_DEV_NO_PROXY}${_QA_NO_PROXY}${TWISTLOCK_CONSOLE_FQDN},${TWISTLOCK_CONSOLE_FQDN}:8083'"
	
	export NO_PROXY=${_DEV_NO_PROXY}${_QA_NO_PROXY}${TWISTLOCK_CONSOLE_FQDN},${TWISTLOCK_CONSOLE_FQDN}:8083
}
